 int main() {
     int firstNumber, secondNumber;
     
     cout << "Введите первое целое число: ";
     cin >> firstNumber;
     
     cout << "Введите второе целое число: ";
     cin >> secondNumber;
     
     if (firstNumber < -100 || firstNumber > 99 || 
         secondNumber < -100 || secondNumber > 99) {
         cout << "Ошибка: Одно из чисел вне диапазона!" << endl;
         return 1;
     }
     
     string comparisonResult;
     if (firstNumber > secondNumber) {
         comparisonResult = getNumberText(firstNumber) + " больше чем " + getNumberText(secondNumber);
     } else if (firstNumber < secondNumber) {
         comparisonResult = getNumberText(firstNumber) + " меньше чем " + getNumberText(secondNumber);
     } else {
         comparisonResult = getNumberText(firstNumber) + " равно " + getNumberText(secondNumber);
     }
     
     cout << "\nРезультат сравнения: " << comparisonResult << endl;
     
     return 0;
 }
